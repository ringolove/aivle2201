package chap05;

import java.util.Random;
public class Lotto {
	public static void main(String[] args) {
	Random random = new Random();
	random.setSeed(System.currentTimeMillis());
	System.out.print((random.nextInt(45)+1)+", ");
	System.out.print((random.nextInt(45)+1)+", ");
	System.out.print((random.nextInt(45)+1)+", ");
	System.out.print((random.nextInt(45)+1)+", ");
	System.out.print((random.nextInt(45)+1)+", ");
	System.out.print((random.nextInt(45)+1));
	}
}
