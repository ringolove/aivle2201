package chap03;

public class Test2{
	class Electronics{
		boolean powerOn;
	}
	
	class TV extends Electronics{
		int volume;
		public int getVolume() {return volume;}
	}
	
	public class Test{
	public void main(String[] args) {
		TV tv = new TV();
		System.out.println(tv.powerOn+" "+tv.volume);

		}
	}
}

